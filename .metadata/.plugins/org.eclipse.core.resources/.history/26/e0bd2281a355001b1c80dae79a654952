	public static void main(String[] args) {
		
		 /**Formas diferentes de criar objetos**/
		
		/* Primeiro metodo*/
		Conta contaDaMaria = new Conta();
		/*Segundo Metodo*/
		Conta contaDoJoao;
		contaDoJoao = new Conta();
		
		/*Determinar os dados da conta da Maria*/
		contaDaMaria.numero = "100-85";
